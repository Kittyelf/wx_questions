/* list */
.list-content{
	margin: 5px 0;
	/* border: 1px solid; */
	height: 80px;
	display: -webkit-box;
	padding: 15px 10px;
	border-top: 1px #eeeeee solid;
}
.cont-l{
	width: 80%;
}
.cont-l>view:nth-of-type(1){
	height: 40px;
	font-size: 20px;
    overflow: hidden;/*超出部分隐藏*/
    white-space: nowrap;/*不换行*/
    text-overflow:ellipsis;/*超出部分文字以...显示*/
    line-height: 40px;
}
.cont-l>view:nth-of-type(2){
	/* width: 80%; */
	height: 40px;
	font-size: 14px;
	line-height: 40px;
	overflow: hidden;
	position: relative;
	top: -5px;
}
.cont-l>view .p{
	display: inline-block;
	background-color: #F5F5F5;
	border-radius: 5px;
	line-height: 20px;
	padding: 3px 8px;
	margin-left: 10px;
}
.cont-r{
	width: 23%;
}
.cont-r>view:nth-of-type(1){
	color: #FF000F;
	font-size: 26px;
	text-align: center;
}
.cont-r>view:nth-of-type(2){
	color: #808080;
	/* font-weight: bold; */
